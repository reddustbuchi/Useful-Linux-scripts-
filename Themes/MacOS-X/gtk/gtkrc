	file            = "hline.png"
	border          = { 0, 0, 1, 1 }
	stretch         = TRUE
      }
    image
      {
        function        = VLINE
	recolorable     = TRUE
	file            = "vline.png"
	border          = { 1, 1, 0, 0 }
	stretch         = TRUE
      }
