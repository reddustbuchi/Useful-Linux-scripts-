	border          = { 15,15,10,9 }
	stretch         = TRUE
      }
    image
      {
        function        = BOX
	recolorable     = FALSE
	state           = INSENSITIVE
	shadow          = OUT
	file            = "button13.png"
	border          = { 15,15,10,9 }
	stretch         = TRUE
      }
