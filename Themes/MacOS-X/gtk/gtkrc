	stretch         = TRUE
      }
  }
}

# The rounded buttons are transparent round the edges. I find when they are used in application without anything behind the buttons, that the transparent parts of
# button12alpha.png and button11alpha.png show garbage . The only application I've had this happen with is XChat so its probably just its problem.
# I have edited button12.png and button11.png so that there is no transparent part round the edges. This does not look so clean but avoids the problem. If you have the
# problem then just edit button12alpha.png and button11alpha.png to be button12.png and button11.png respectively below.
#
# If anyone has this problem and manages to fix it please mail me at tenpin22@blueyonder.co.uk.
# If you have no idea what I'm talking about just ignore me =D.

class "GtkToggleButton" style "togglebutton"

style "button"
{
  bg[NORMAL]      = "#e7e7e7"
  bg[PRELIGHT]    = "#e7e7e7"
  bg[ACTIVE]      = "#e7e7e7"
  bg[INSENSITIVE] = "#e7e7e7"
  bg[SELECTED]    = "#e7e7e7"
  bg[SELECTED]    = "#e7e7e7"

  engine "pixmap"
    {
    image
      {
        function        = BOX
	recolorable     = FALSE
	state           = NORMAL
	shadow          = OUT
	file            = "button12.png"
	border          = { 15,15,10,9 }
	stretch         = TRUE
      }
