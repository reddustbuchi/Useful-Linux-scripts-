################################################################################
# SPINBUTTONS
################################################################################

style "spinbutton" = "entry"
{
        engine "pixmap"
  	{
  		# Arrow
    	image
    	{
     		function	= ARROW
    	}

		# Up
		image
		{
		    function		= BOX
		    state 		= NORMAL
		    detail		= "spinbutton_up"
		    recolorable		= TRUE
		    file		= "Spin/spinbutton-up.png"
		    border		= { 0, 0, 0, 0 }
		    stretch		= TRUE
		    overlay_file	= "Spin/spin-up.png"
		    overlay_stretch	= FALSE
		}

		# Up (mouse over)
		image
		{
