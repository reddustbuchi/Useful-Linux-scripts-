    	{
       		function	= SHADOW
	        shadow		= ETCHED_IN
	        recolorable	= TRUE
 	        file		= "Frame-Gap/frame1.png"				
	        border		= { 10, 10, 10, 10 }
	        stretch		= TRUE
    	}

        image 
      	{
       		function        = SHADOW
 		recolorable     = TRUE
		shadow          = ETCHED_OUT
		file		= "Others/transparent.png"
