        }

	image 
	{
            function        = SLIDER 
            recolorable     = TRUE
            state           = INSENSITIVE
            file            = "Scrollbars/scrollbar_horizontal_insens.png"
            border          = { 11, 11, 6, 6 }
            stretch         = TRUE
            orientation     = HORIZONTAL
        }
      
        image 
	{
            function        = SLIDER 
            recolorable     = TRUE
