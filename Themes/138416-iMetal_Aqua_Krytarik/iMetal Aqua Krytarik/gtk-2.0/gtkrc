################################################################################
# SHADOWS
################################################################################

	 image
	 {
	       function		= SHADOW
	       shadow		= NONE
	       recolorable	= TRUE
	       file		= "Others/transparent.png"
	       border		= { 0, 0, 0, 0 }
	       stretch		= TRUE
	 }

    	image 
      	{
       		function        = SHADOW
 		recolorable     = TRUE
		shadow          = OUT
		file            = "Shadows/shadow-out.png"
		border          = { 2, 2, 2, 2 }
		stretch         = TRUE
      	}	

    	image 
      	{
       		function        = SHADOW
 		recolorable     = TRUE
		shadow          = IN
		file            = "Shadows/shadow-in.png"
		border          = { 2, 2, 2, 2 }
		stretch         = TRUE
      	}	

