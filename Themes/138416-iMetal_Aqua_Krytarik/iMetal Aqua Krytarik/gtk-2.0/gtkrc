################################################################################
# ENTRY
################################################################################

style "entry" = "default"
{

	base[INSENSITIVE] = "#bbbbba"

	xthickness = 2
	ythickness = 2
  
	GtkWidget::interior_focus	= 0

	engine "pixmap"
	{
		# Entry (focused)
		image
		{
			function		= FOCUS
			recolorable		= TRUE
			file			= "Shadows/entry-shadow-in.png"
			border			= { 4,4,4,4 }
			stretch			= TRUE
		}
		
		
		# Text-entry 
		image
		{
			function		= BOX
			recolorable		= TRUE
			shadow			= OUT
			state			= NORMAL
			file			= "Shadows/text-entry.png"
			border			= { 4,4,4,4 }
			stretch			= TRUE
		}	

		# Text-entry
		image
		{
			function		= SHADOW
			detail			= "entry"
			shadow			= IN
			recolorable		= TRUE
			file			= "Shadows/text-entry.png"
			border			= { 4,4,4,4 }
			stretch			= TRUE
		}
	}
}

